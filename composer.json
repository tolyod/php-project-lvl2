        "bin/gendiff"
    ],
    "description": "Apoloz simple diff generator",
    "type": "project",
    "license": "MIT",
    "authors": [
      {
        "name": "Anatoliy Poloz",
        "email": "anatoliy.poloz@gmail.com"
      }
